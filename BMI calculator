weight = float(input("Enter your weight: "))
height = float(input("Enter your height: "))

bmi = weight*703 / (height ** 2)

print("Your BMI: ", bmi)
if bmi>=18.5 and bmi<=25:
    print("You have optimal weight!")
elif bmi < 18.5:
    print("You are underweight!")
else:
    print("You are overweight!")

